import java.util.*;

public class Count{

public static void main(String[] args){

Scanner sc1=new Scanner(System.in);

System.out.print("enter a string:\n");

String s1=sc1.nextLine();

int[] cnt=new int[256];

for(int i=0;i<s1.length();i++){

cnt[(int)s1.charAt(i)]++;

}

for(int i=0;i<256;i++){

if (cnt[i]!=0){

System.out.print((char)i+" : "+cnt[i]+"\n");

}

}

}

}
