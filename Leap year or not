import java.util.*;

public class LpYr{

public static void main(String[] args){

Scanner sc1=new Scanner(System.in);

System.out.println("enter the year:");

int y=sc1.nextInt();

if ((y%4==0)&&(y%100!=0)){

System.out.println("Leap year");

}

else if(y%400==0){

System.out.println("Leap year");

}

else{

System.out.println("Not a Leap year");

}

}

}

