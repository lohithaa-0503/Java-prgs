import java.util.*;
public class PatternPrint_2{
public static void main(String[] args){
Scanner sc=new Scanner(System.in);
System.out.print("Enter the limit\n");
int n=sc.nextInt();
char c='A';
for(int i=1;i<=n;i++){
int c1=(int)c;
for(int j=1;j<=i;j++,c1++){
System.out.print((char)c1);
}
System.out.print("\n");
}
}
}
