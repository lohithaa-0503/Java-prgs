import java.util.*;
public class SentenceReverse{
public static void main(String[] args){
Scanner sc1=new Scanner(System.in);
String s=sc1.nextLine();
int x=s.length();
for(int i=s.length()-1;i>=0;i--){
if(s.charAt(i)==' '){
for(int j=i+1;j<x;j++){
System.out.print(s.charAt(j));
}
System.out.print(" ");
x=i;
}
else if(i==0){
for(int j=i;j<x;j++){
System.out.print(s.charAt(j));
}
}
}
System.out.print("\n");
}
}
