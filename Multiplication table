import java.util.*;
public class Multiply_table{
public static void main(String[] args){
Scanner sc1=new Scanner(System.in);
System.out.print("enter a no:\n");
int n=sc1.nextInt();
System.out.print("enter the limit:\n");
int l=sc1.nextInt();
for(int i=1;i<=l;i++){
System.out.print(n+"*"+i+"="+(n*i)+"\n");
}
}
}
