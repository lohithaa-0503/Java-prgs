import java.util.*;
public class Prime{
public static void main(String[] args){
Scanner sc1=new Scanner(System.in);
System.out.print("Enter a no:\n");
int n=sc1.nextInt();
int flag=0;
for(int i=2;i<n/2;i++){
if (n%i==0){
System.out.print(n+" is not a Prime no");
flag=1;
break;
}
}
if(flag==0){
System.out.print(n+" is a Prime no");
}
}
}
