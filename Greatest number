import java.util.*;

public class Grtr{

public static void main(String[] args){

Scanner sc1=new Scanner(System.in);

System.out.println("no1:");

int n1=sc1.nextInt();

System.out.println("no2:");

int n2=sc1.nextInt();

System.out.println("no3:");

int n3=sc1.nextInt();

int r;

r=(n1>n2)?((n1>n3)?n1:n3):((n2>n3)?n2:n3);

System.out.println(r+" is greater");

}

}
